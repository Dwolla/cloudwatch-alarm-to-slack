{
    "compilerOptions": {
      "esModuleInterop": true,
      "lib": ["esnext"],
      "moduleResolution": "node",
      "noFallthroughCasesInSwitch": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "resolveJsonModule": true,
      "sourceMap": true,
      "target": "es2018"
    }
  }